{
  "name": "root",
  "private": true,
  "version": "1.0.4",
  "scripts": {
    "dev": "pnpm run build:light && pnpm run -r --parallel dev",
    "build": "pnpm -r build",
    "build:light": "pnpm -F=vue3-sfc-transitions run build:light",
    "lint": "npm run lint:es; npm run lint:style",
    "lint:es": "pnpm -r --parallel --no-bail lint:es",
    "lint:style": "pnpm -r --parallel --no-bail lint:style",
    "test": "pnpm -r --parallel test",
    "test:coverage": "pnpm -r --parallel run test:coverage"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-vue": "^9.9.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.9.4"
  }
}
