    "src"
  ],
  "license": "MIT",
  "main": "dist/vue3-sfc-transitions.umd.js",
  "module": "dist/vue3-sfc-transitions.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite build --watch",
